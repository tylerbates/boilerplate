<config>
    <modules>
        <Oggetto_Shipping>
            <version>0.1.0</version>
        </Oggetto_Shipping>
    </modules>
    <global>
        <models>
            <oggetto_shipping>
                <class>Oggetto_Shipping_Model</class>
            </oggetto_shipping>
        </models>
        <resources>
            <oggetto_shipping_setup>
                <setup>
                    <module>Oggetto_Shipping</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </oggetto_shipping_setup>
        </resources>
    </global>
    <default>
        <carriers>
            <oggetto>
                <active>1</active>
                <name>Oggetto Shipping</name>
                <title>Oggetto Shipping</title>
                <model>oggetto_shipping/carrier_oggetto</model>
            </oggetto>
        </carriers>
    </default>
</config>